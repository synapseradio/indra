# Valid INDRA code for testing verify command

@command:
  you:
    possess:
      identifier: TEST_VALID
      state:
        counter: 0
    identity: "valid test command"
    rules:
      - "demonstrate correct INDRA structure"
      - "follow all specification rules"
    understands:
      - "this is a valid example"
      - "all rules are followed"
    
    respond:
      on: test_message
      you:
        possess:
          identifier: TEST_HANDLER
        identity: "test message handler"
        rules: ["handle test message"]
        understands: ["message handling"]
        perform:
          method: "test action"
          output: <<Handling test message...>>
          then:
            emit: test_complete

    respond:
      on: test_complete
      you:
        possess:
          identifier: COMPLETE_HANDLER
        identity: "completion handler"
        rules: ["acknowledge completion"]
        understands: ["flow completion"]
        perform:
          method: "acknowledgment"
          output: <<Test completed successfully>>